{
  "$schema": "../../../node_modules/.pnpm/@tauri-apps+cli@2.0.0-alpha.17/node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "distDir": ".",
    "devPath": "http://localhost:4000"
  },
  "tauri": {
    "bundle": {
      "identifier": "studio.tauri.example",
      "active": true,
      "icon": ["../../../examples/api/src-tauri/icons/icon.png"]
    },
    "windows": [
      {
        "title": "Tauri App"
